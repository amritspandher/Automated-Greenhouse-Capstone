<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.automatedgreenhouse">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:name=".User"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AutomatedGreenhouse">
        <activity
            android:name=".LoginPage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage"></activity>
        <activity android:name=".RegisterPage"></activity>
        <activity android:name=".AddNewSystem"></activity>
        <activity android:name=".MainDisplay"></activity>
        <activity android:name=".SchedulePage"></activity>
        <activity android:name=".ModulePage"></activity>
        <activity android:name=".ModulePage2"></activity>
        <activity android:name=".AddNewModule"></activity>
        <activity android:name=".SchedulePageCreate"></activity>
        <activity android:name=".StatsPage"
            android:exported="true"></activity>
        <activity android:name=".CameraPage"></activity>
    </application>

</manifest>